<Package>
  <DisplayName>Bonjour</DisplayName>
  <Description>
    This is Needed component to discover the
    Devices on your network, you may
    deselect if you already have one
  </Description>
  <Version>3.0.0</Version>
  <ReleaseDate>2023-07-19</ReleaseDate>
  <RequiresAdminRights>true</RequiresAdminRights>
  <Operations>
    <Operation name="Execute">
      <Argument>msiexec</Argument>
      <Argument>/fo</Argument>
      <Argument>@TargetDir@/bonjour.msi</Argument>
    </Operation>
  </Operations>
</Package>
